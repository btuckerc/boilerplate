{
    "$schema": "vscode://schemas/color-theme",
    "name": "Adwaita dark",
    "type": "dark",
    "semanticHighlighting": true,
    "colors": {
        "editor.background": "#000000",
        "editor.foreground": "#000000",
        "editor.selectionBackground": "#000000",
        "editor.selectionForeground": "#000000",
        "editorCursor.foreground": "#000000",
        "editorCursor.background": "#000000",
        "terminal.ansiBlack": "#000000",
        "terminal.ansiRed": "#000000",
        "terminal.ansiGreen": "#000000",
        "terminal.ansiYellow": "#000000",
        "terminal.ansiBlue": "#000000",
        "terminal.ansiMagenta": "#000000",
        "terminal.ansiCyan": "#000000",
        "terminal.ansiWhite": "#000000",
        "terminal.ansiBrightBlack": "#000000",
        "terminal.ansiBrightRed": "#000000",
        "terminal.ansiBrightGreen": "#000000",
        "terminal.ansiBrightYellow": "#000000",
        "terminal.ansiBrightBlue": "#000000",
        "terminal.ansiBrightMagenta": "#000000",
        "terminal.ansiBrightCyan": "#000000",
        "terminal.ansiBrightWhite": "#000000",
        "activityBar.background": "#000000",
        "activityBar.foreground": "#000000",
        "sideBar.background": "#000000",
        "sideBar.foreground": "#000000",
        "statusBar.background": "#000000",
        "statusBar.foreground": "#000000",
        "titleBar.activeBackground": "#000000",
        "titleBar.activeForeground": "#000000",
        "titleBar.inactiveBackground": "#000000",
        "titleBar.inactiveForeground": "#000000",
        "editorGroupHeader.tabsBackground": "#000000",
        "tab.activeBackground": "#000000",
        "tab.inactiveBackground": "#000000",
        "tab.activeForeground": "#000000",
        "tab.inactiveForeground": "#000000",
        "list.activeSelectionBackground": "#000000",
        "list.activeSelectionForeground": "#000000",
        "list.inactiveSelectionBackground": "#000000",
        "list.inactiveSelectionForeground": "#000000",
        "list.hoverBackground": "#000000",
        "list.hoverForeground": "#000000"
    },
    "tokenColors": [
        {
            "scope": ["comment"],
            "settings": {
                "foreground": "#000000",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["string", "string.quoted", "string.quoted.single", "string.quoted.double"],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": ["variable", "variable.other", "variable.parameter", "variable.language"],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": ["constant", "constant.numeric", "constant.language", "constant.character"],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": ["entity.name.function", "support.function"],
            "settings": {
                "foreground": "#000000"
            }
        }
    ]
}
