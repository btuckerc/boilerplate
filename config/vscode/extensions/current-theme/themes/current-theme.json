{
    "$schema": "vscode://schemas/color-theme",
    "name": "Current Theme",
    "type": "dark",
    "semanticHighlighting": true,
    "colors": {
        "editor.background": "#1d1d1d",
        "editor.foreground": "#deddda",
        "editor.selectionBackground": "#303030",
        "editor.selectionForeground": "#c0bfbc",
        "editorCursor.foreground": "#deddda",
        "editorCursor.background": "#1d1d1d",
        "terminal.ansiBlack": "#1d1d1d",
        "terminal.ansiRed": "#ed333b",
        "terminal.ansiGreen": "#57e389",
        "terminal.ansiYellow": "#ff7800",
        "terminal.ansiBlue": "#62a0ea",
        "terminal.ansiMagenta": "#9141ac",
        "terminal.ansiCyan": "#5bc8af",
        "terminal.ansiWhite": "#deddda",
        "terminal.ansiBrightBlack": "#9a9996",
        "terminal.ansiBrightRed": "#f66151",
        "terminal.ansiBrightGreen": "#8ff0a4",
        "terminal.ansiBrightYellow": "#ffa348",
        "terminal.ansiBrightBlue": "#99c1f1",
        "terminal.ansiBrightMagenta": "#dc8add",
        "terminal.ansiBrightCyan": "#93ddc2",
        "terminal.ansiBrightWhite": "#f6f5f4",
        "activityBar.background": "#1d1d1d",
        "activityBar.foreground": "#deddda",
        "sideBar.background": "#1d1d1d",
        "sideBar.foreground": "#deddda",
        "statusBar.background": "#62a0ea",
        "statusBar.foreground": "#f6f5f4",
        "titleBar.activeBackground": "#1d1d1d",
        "titleBar.activeForeground": "#deddda",
        "titleBar.inactiveBackground": "#1d1d1d",
        "titleBar.inactiveForeground": "#9a9996",
        "editorGroupHeader.tabsBackground": "#1d1d1d",
        "tab.activeBackground": "#303030",
        "tab.inactiveBackground": "#1d1d1d",
        "tab.activeForeground": "#deddda",
        "tab.inactiveForeground": "#9a9996",
        "list.activeSelectionBackground": "#303030",
        "list.activeSelectionForeground": "#deddda",
        "list.inactiveSelectionBackground": "#1d1d1d",
        "list.inactiveSelectionForeground": "#deddda",
        "list.hoverBackground": "#303030",
        "list.hoverForeground": "#deddda"
    },
    "tokenColors": [
        {
            "scope": ["comment"],
            "settings": {
                "foreground": "#9a9996",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["string", "string.quoted", "string.quoted.single", "string.quoted.double"],
            "settings": {
                "foreground": "#57e389"
            }
        },
        {
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#9141ac"
            }
        },
        {
            "scope": ["variable", "variable.other", "variable.parameter", "variable.language"],
            "settings": {
                "foreground": "#62a0ea"
            }
        },
        {
            "scope": ["constant", "constant.numeric", "constant.language", "constant.character"],
            "settings": {
                "foreground": "#ff7800"
            }
        },
        {
            "scope": ["entity.name.function", "support.function"],
            "settings": {
                "foreground": "#5bc8af"
            }
        }
    ]
}
